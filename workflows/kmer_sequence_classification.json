{
    "__description__": "Parameters for classification of sequences based on k-mer positional profiles.",
    "__author__": "Jacques van Helden",

    "qsub": "-V -m a -d /Users/jvanheld/BeatriceRoche/",
    "verbosity": "2",

    "dir": {
	"results": "results"
    },

